#header
    background: url('https://web-storage-1257894433.cos.ap-shanghai.myqcloud.com/holdyourhand/base/web-bg.png')
    padding-bottom: nav-height
    position: relative

#header-outer
    height: 100%
    position: relative

#header-inner
    position: relative

#header-title
    clearfix()
    text-align: center
    padding: 30px 30px
    background-size: auto 240px
    .logo-wrap, .subtitle-wrap
        float: left
    .subtitle-wrap
        padding: 10px 0px 0px
        margin-left: 20px
    .subtitle
        font-size: 16px
        font-style: italic
        line-height: 30px
        color: white
        text-shadow: 0 1px rgba(0, 0, 0, 0.2)

